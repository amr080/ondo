{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/x/ondo.finance",
      "destination": "/x/ondo.finance/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
